class Solution {
    public int climbStairs(int n) {
        if(n==1 || n==2 || n==3){
            return n;
        }
        int curr = 1;
        int prev = 0;
        for(int i=0;i<n;i++){
            int temp = curr;
            curr+=prev;
            prev = temp;
        }
        return curr;
    }
}
